function checkSize(){var width=window.innerWidth;if(width<980){var navBar=document.getElementById("header-wrapper");var toggle_open=document.getElementById("navbar-toggler");var space_decktop=document.getElementById("decktop");navBar.style.display="none";space_decktop.style.display="none";toggle_open.style.display="block"}else if(width>=980){var navBar=document.getElementById("header-wrapper");var toggle_open=document.getElementById("navbar-toggler");var space_decktop=document.getElementById("decktop");
navBar.style.display="block";space_decktop.style.display="block";toggle_open.style.display="none"}}function checkNav(){var url=window.location.href;var subj_url=document.getElementById("layout_8");var subj_menu=document.getElementById("navigation_menu");if(url===subj_url.href)subj_menu.style.display="block";else subj_menu.style.display="none"};